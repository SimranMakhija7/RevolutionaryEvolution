<% include partials/header %>
<%include partials/show_header%>
 <div class="container">
 <div class="row">
    <div class="col-md-4 col-sm-6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">What??</span>
          <p><%=data.timeline[0].what%></p>
        </div>
      </div>
    </div>
    <div class="col-md-4 col-sm-6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">When??</span>
          <p><%=data.timeline[0].when%></p>
        </div>
      </div>
    </div>
    <div class="col-md-4 col-sm-6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">What did they eat?</span>
          <p><%=data.timeline[0].food%></p>
        </div>
      </div>
    </div>
    <div class="col-md-4 col-sm-6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">How did they look?</span>
          <p><%=data.timeline[0].size%></p>
        </div>
      </div>
    </div>
    <div class="col-md-4 col-sm-6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">How did they come to be?</span>
          <p><%=data.timeline[0].evolution%></p>
        </div>
      </div>
    </div>
    <div class="col-md-4 col-sm-6">
      <div class="card blue-grey darken-1">
        <div class="card-image waves-effect waves-block waves-light">
            <img class="activator" src="<%=data.coverImageURL%>">
        </div>
      <div class="card-content">
        <span class="card-title activator grey-text text-darken-4">Some general traits<i class="material-icons right"></i></span>
      </div>
      <div class="card-reveal">
        <span class="card-title grey-text text-darken-4">General Traits<i class="material-icons right">close</i></span>
        <p>
          <ul>
            <%data.timeline[0].traits.forEach(function(trait){%>
                <li><%=trait%></li>
            <%});%>
          </ul>
        </p>
      </div>
  </div>
</div>
</div>
</div>

<% include partials/footer %>